{
  "name": "proseva-sdk",
  "version": "0.1.1",
  "description": "Type-safe client for the ProSeVA API generated from openapi.json",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "generate": "openapi-typescript ../server/openapi.json --output src/types.ts",
    "build": "tsc -p tsconfig.json",
    "prepare": "npm run generate && npm run build"
  },
  "dependencies": {
    "openapi-fetch": "^0.15.0"
  },
  "devDependencies": {
    "openapi-typescript": "^7.10.1",
    "typescript": "~5.9.3"
  }
}
