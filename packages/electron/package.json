{
  "name": "@proseva/electron",
  "version": "0.0.0-prerelease",
  "private": true,
  "main": "dist/main.js",
  "scripts": {
    "dev": "cd ../gui && bun run build && cd ../electron && tsc && bun build src/preload.ts --target=node --format=cjs --external=electron --outfile=dist/preload.js && electron .",
    "build": "tsc && bun build src/preload.ts --target=node --format=cjs --external=electron --outfile=dist/preload.js",
    "typecheck": "tsc --noEmit",
    "test": "echo 'No tests yet'",
    "dist": "bun run build && electron-builder",
    "dist:local": "bun run build && electron-builder --mac --arm64"
  },
  "dependencies": {
    "@proseva/gui": "workspace:*"
  },
  "devDependencies": {
    "@electron/notarize": "^3.1.1",
    "@types/bun": "latest",
    "@types/node": "^24.10.1",
    "electron": "34.5.8",
    "electron-builder": "^26.0.12",
    "typescript": "~5.9.3"
  },
  "peerDependencies": {
    "typescript": "^5"
  }
}
