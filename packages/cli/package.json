{
  "name": "@proseva/cli",
  "version": "0.0.0-prerelease",
  "description": "CLI for managing proseva remotely",
  "type": "module",
  "bin": {
    "proseva": "bin/proseva.ts"
  },
  "scripts": {
    "generate-types": "openapi-typescript ../../server/openapi.json -o lib/api-types.d.ts",
    "build": "bun build bin/proseva.ts --outdir dist --target node",
    "dev": "bun run bin/proseva.ts",
    "test": "bun test"
  },
  "dependencies": {
    "@proseva/sdk": "workspace:*",
    "chalk": "^5.3.0",
    "cli-table3": "^0.6.5",
    "commander": "^12.0.0",
    "openapi-fetch": "^0.12.2",
    "ora": "^8.1.1"
  },
  "devDependencies": {
    "openapi-typescript": "^7.4.3",
    "@types/node": "^20.0.0"
  }
}
